@model WorkFlowManager.Common.ViewModels.TestWorkFlowFormViewModel


<div class="box">

    <div class="box-header with-border">
        <div class="col-lg-12">
            <h3 class="box-title">Age Analyze Form</h3>
        </div>
    </div>
    <div class="box-body">
        @Html.HiddenFor(x => x.OwnerId)
        <div class="form-group">
            @Html.LabelFor(x => x.Age, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-9">
                @Html.TextBoxFor(x => x.Age, new { @class = "form-control",  @type = "number"  })
                @Html.ValidationMessageFor(x => x.Age)
            </div>
        </div>
    </div>
</div>