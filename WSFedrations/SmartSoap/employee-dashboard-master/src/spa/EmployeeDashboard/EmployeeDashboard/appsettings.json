{
  "Logging": {
    "Serilog": {
      "PathFormat": "Logs/log-{Date}.txt",
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Information"
      }
    },
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
